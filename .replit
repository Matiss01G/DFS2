
modules = ["cpp"]

[nix]
channel = "stable-24_05"

[workflows]
runButton = "Build"

[[workflows.workflow]]
name = "Build"
mode = "sequential"

[[workflows.workflow.tasks]]
task = "packager.installForAll"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd build && cmake .. && make"

[[ports]]
localPort = 12345
externalPort = 80
